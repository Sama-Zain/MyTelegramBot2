modules = ["python-3.11"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "TelegramBot"

[[workflows.workflow]]
name = "TelegramBot"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd MyTelegramBot && python main_replit.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 38215
externalPort = 3000

[deployment]
deploymentTarget = "vm"
run = ["python", "MyTelegramBot/main_replit.py"]
